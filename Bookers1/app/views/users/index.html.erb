<div class="container">
<div class="col-xs-3">
<%= render 'users/info', user: @user %>
<%= render 'books/new', book: @book %>
</div>
<div class="col-xs-9">
<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</table>
  <tbody>
  	<% @users.each do |user| %>
  	<div class="user">
      <tr>
        <td><!--img class="" alt="No image icon"--></td>
        <td><%= user.name %></td>
        <td><%= user.introduction %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
  </div>
    <% end %>
  </tbody>
</div>
</div>